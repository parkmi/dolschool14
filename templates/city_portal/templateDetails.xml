<params>
		<param name="spacer1" type="spacer" default="Make sure to hover your mouse over each title in this column for a full description of what each field does." />
		<param name="xml_menuname" type="text" default="mainmenu" label="Menu Name" description="What menu would you like to use for the default default menu?" />
		<param name="xml_s5_body_width" type="text" default="975" label="Body Width" description="What width would you like for the website?" />
		<param name="xml_s5_left_width" type="text" default="200" label="Left Column Width" description="What width in pixels would you like the left column to be?" />
		<param name="xml_s5_right_width" type="text" default="275" label="Right Column Width" description="What width in pixels would you like the right column to be?" />	
		<param name="xml_s5_menu" type="list" default="1" label="Menu Type" description="Enter in 1 for No-MooMenu, 2 for Suckerfish, 3 for Fading No-Moo, or 4 for Nothing">
			<option value="1">S5 Drop In No-MooMenu</option>
			<option value="2">Suckerfish</option>
			<option value="3">S5 Fading No-MooMenu</option>
			<option value="4">No Menu</option>
		</param>	
		<param name="xml_s5_color" type="text" default="default" label="Color Scheme" description="Enter what color: default, orange or blue you would like to set the template to.  This will change the menu, logo and read more color." />
		<param name="xml_s5_box_width" type="text" default="300" label="S5 Box width" description="What width in pixels would you like the S5 Box to be?" />

		<param name="xml_s5_tab1" type="text" default="Register" label="S5 Box Label" description="Enter what sub text you would like for the S5 Box tab" />	
		<param name="xml_s5_registerlink" type="text" default="" label="Right Tab Link" description="Enter where the top right tab should link to" />		
		<param name="xml_s5_repeatback" type="text" default="images/s5_cityportal_backrepeat.jpg" label="Background Repeat" description="Enter the URL to a background image that will repeat throughout the body of the template" />
		<param name="xml_s5_colorback" type="text" default="#FFFFFF" label="Color Background" description="Enter the color in HEX of the background you would like for the template body" />
		<param name="xml_s5_tooltips" type="text" default="yes" label="Tooltips" description="Enable tooltips" />
		<param name="xml_s5_lytebox" type="list" default="yes" label="Enable Lytebox?" description="Enable the lytebox feature for your images  NOTE: it is highly recommened to disable if you are not planning to use">
			<option value="yes">yes</option>
			<option value="no">no</option>
		</param>	
		<param name="xml_s5_clr_fix" type="list" default="disabled" label="S5 Clr Fix" description="S5 - Clear Fix Script - This template was written with 100% tableless css. Unfortunately there are some components that are poorly written and conflict with tableless designs this is an issue with the component and not the template. To correct some of these issues we have written this simple script. If you are seeing a large gap above some of your components you can try enabling this script (choose enabled or disabled - only use this script if absolutly necessary). In most cases this will fix the issue but not all. If you are still experiencing the problem this is most likely a problem with the component.">
			<option value="disabled">disabled</option>
			<option value="enabled">enabled</option>
		</param>
	
		
</params>
